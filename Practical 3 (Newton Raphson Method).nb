Cell[TextData[{
 "Q1. Find approximate root of the equation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "4"], "-", " ", "x", " ", "-", " ", "10"}], " ", "=",
     "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nSolution:"
}], "Subtitle",
