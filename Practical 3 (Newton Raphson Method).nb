Cell[CellGroupData[{

Cell[TextData[{
 "Q2. Find the approximate root of the following:\ni) 3x - Cos(x) - 1\nii) \
Cosx - ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["xe", "x"], TraditionalForm]]],
 "=0\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"iii", ")"}], 
     SuperscriptBox["e", 
      RowBox[{"-", "x"}]]}], "=", "x"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nSolution:\ni)3x - Cos(x) - 1"
}], "Subtitle",
 CellChangeTimes->{{3.9161814029277573`*^9, 3.916181456429818*^9}, {
  3.916181508474795*^9, 3.916181515524198*^9}, {3.916182391929325*^9, 
  3.916182401832892*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"3", "x"}], " ", "-", " ", 
   RowBox[{"Cos", "[", "x", "]"}], "-", "1"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["x", "0"], "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"e", "=", " ", 
   RowBox[{"5", "*", 
    SuperscriptBox["10", 
     RowBox[{"-", "5"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"iterations", "=", "5"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"n", "=", "1"}], ",", 
    RowBox[{"n", "\[LessEqual]", "iterations"}], ",", 
    RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "n"], "=", " ", 
      RowBox[{"N", "[", 
       RowBox[{
        SubscriptBox["x", 
         RowBox[{"n", "-", "1"}]], "-", " ", 
