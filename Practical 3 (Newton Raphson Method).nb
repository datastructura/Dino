  3.916180470903449*^9, 3.9161804765127697`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"x", "^", "4"}], " ", "-", " ", "x", " ", "-", " ", 
   "10"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["x", "0"], "=", "2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"e", "=", " ", 
   RowBox[{"5", "*", 
    SuperscriptBox["10", 
     RowBox[{"-", "5"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"iterations", "=", "5"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"n", "=", "1"}], ",", 
    RowBox[{"n", "\[LessEqual]", "iterations"}], ",", 
    RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "n"], "=", " ", 
      RowBox[{"N", "[", 
       RowBox[{
        SubscriptBox["x", 
         RowBox[{"n", "-", "1"}]], "-", " ", 
        FractionBox[
         RowBox[{"f", "[", 
          SubscriptBox["x", 
           RowBox[{"n", "-", "1"}]], "]"}], 
         RowBox[{
          RowBox[{"f", "'"}], "[", 
          SubscriptBox["x", 
           RowBox[{"n", "-", "1"}]], "]"}]]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{
          SubscriptBox["x", "n"], "-", 
          SubscriptBox["x", 
           RowBox[{"n", "-", "1"}]]}], "]"}], "<", "e"}], ",", 
       RowBox[{"Return", "[", 
        SubscriptBox["x", "n"], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"n", ",", "\"\<th iteration's value is: \>\"", ",", 
       SubscriptBox["x", "n"]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Estimated error is: \>\"", ",", 
       RowBox[{"Abs", "[", 
        RowBox[{
         SubscriptBox["x", "n"], "-", 
         SubscriptBox["x", 
          RowBox[{"n", "-", "1"}]]}], "]"}]}], "]"}]}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<Final approximate root is: \>\"", ",", 
   SubscriptBox["x", "n"]}], "]"}], "\[IndentingNewLine]", 
