Cell["\<\

iii) Using Euler\[CloseCurlyQuote]s method find an approximate value of y \
corresponding x = 1.2, for the first order ODE f(x,y) =log(x+y) and y = 2 at \
x = 0.\
\>", "Subtitle",
 CellChangeTimes->{{3.921019751317083*^9, 3.9210197535722117`*^9}, {
  3.921019953411642*^9, 3.921019954125683*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"Log", "[", 
    RowBox[{"x", "+", "y"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"a", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b", "=", "1.2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "10"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"y", "[", "0", "]"}], "=", "2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"h", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"b", "-", "a"}], ")"}], "/", "n"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "0"}], ",", 
   RowBox[{"i", "\[LessEqual]", "n"}], ",", 
   RowBox[{"i", "++"}], ",", 
   RowBox[{
    RowBox[{
     RowBox[{"x", "[", "i", "]"}], "=", 
     RowBox[{"a", "+", 
      RowBox[{"h", "*", "i"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"y", "[", 
      RowBox[{"i", "+", "1"}], "]"}], "=", 
     RowBox[{
      RowBox[{"y", "[", "i", "]"}], "+", 
      RowBox[{"h", "*", 
       RowBox[{"f", "[", 
        RowBox[{
         RowBox[{"x", "[", "i", "]"}], ",", 
         RowBox[{"y", "[", "i", "]"}]}], "]"}]}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\<Value at x[\>\"", ",", "i", ",", "\"\<]=\>\"", ",", 
      RowBox[{"x", "[", "i", "]"}], ",", "\"\< is \>\"", ",", 
      RowBox[{"N", "[", 
       RowBox[{"y", "[", "i", "]"}], "]"}]}], "]"}]}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.9210197734283476`*^9, 3.9210197896392746`*^9}}],

