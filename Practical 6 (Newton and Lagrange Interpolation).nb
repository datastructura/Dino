 RowBox[{"xi", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"-", "1"}], ",", "0", ",", "1", ",", "2"}], 
   "}"}]}], "\[IndentingNewLine]", 
 RowBox[{"fi", "=", " ", 
  RowBox[{"{", 
   RowBox[{"5", ",", "1", ",", "1", ",", "11"}], 
   "}"}]}], "\[IndentingNewLine]", 
 RowBox[{"n", "=", " ", 
  RowBox[{"Length", "[", "xi", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"k", "=", "1"}], ",", 
   RowBox[{"k", "\[LessEqual]", "n"}], ",", 
   RowBox[{"k", "++"}], ",", 
   RowBox[{
    RowBox[{
     SubscriptBox["L", "k"], "[", "x_", "]"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"j", "=", "1"}], 
        RowBox[{"k", "-", "1"}]], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "-", 
          RowBox[{
          "xi", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}]}], 
         ")"}], "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
          "xi", "\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}], "-", 
          RowBox[{
          "xi", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}]}], 
         ")"}]}]}], ")"}], "*", 
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"j", "=", 
         RowBox[{"k", "+", "1"}]}], "n"], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "-", 
          RowBox[{
          "xi", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}]}], 
         ")"}], "/", 
        RowBox[{"(", 
